@using AE.WebUI.ViewModels
@model AccountViewModel

@{
    ViewBag.Title = "Details";
}

<ul class="list-unstyled">
    <li>
        @Model.Message
    </li>
    <li>
        @Model.Username
    </li>
    <li>
        @Model.Email
    </li>
    <li>
        @Html.ActionLink("Change password", "ChangePassword", "Account")
    </li>
</ul>
